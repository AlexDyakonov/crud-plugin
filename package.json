{
  "name": "golang-crud-generator",
  "displayName": "GoLang CRUD generator",
  "description": "CRUD generator for PostgreSQL DB",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "golang-crud-generator.connectToDatabase",
        "title": "Connect to Database"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "GoLang CRUD Generator",
      "properties": {
        "golangCrudGenerator.host": {
          "type": "string",
          "default": "localhost",
          "description": "Database host"
        },
        "golangCrudGenerator.port": {
          "type": "string",
          "default": "5432",
          "description": "Database port"
        },
        "golangCrudGenerator.dbName": {
          "type": "string",
          "default": "",
          "description": "Database name"
        },
        "golangCrudGenerator.user": {
          "type": "string",
          "default": "",
          "description": "Database user"
        },
        "golangCrudGenerator.password": {
          "type": "string",
          "default": "",
          "description": "Database password"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
